{
    "name": "isolate/lazy-objects",
    "description": "Isolate proxy classes generator",
    "keywords": ["isolate", "lazy object", "object", "lazy", "proxy"],
    "require": {
        "ocramius/proxy-manager": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.4.*",
        "phpspec/phpspec": "2.1.*",
        "doctrine/common": "2.4.*",
        "symfony/filesystem": "2.6.*"
    },
    "autoload": {
        "psr-0": {
            "Isolate\\LazyObjects": "src/",
            "Isolate\\LazyObjects\\Tests": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ]
}
